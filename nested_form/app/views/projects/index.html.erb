<%= link_to("To Users", users_path) %>

<h1>Projects</h1>

<%= link_to("Create Project", new_project_path) %>

<table>
  <thead>
    <th>Id</th>
    <th>Name</th>
    <th>Actions</th>
  </thead>

  </tbody>
  <% @projects.each do |p| %>
    <tr>
      <td><%= p.id %></td>
      <td><%= p.name %></td>
      <td>
        <%= link_to("Edit", edit_project_path(p)) %>
        <%= link_to("Delete", project_path(p), :method => :delete) %>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>
